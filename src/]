use std::time::Duration;

pub enum Device {
    OptanePMem,
    OptaneSSD,
    SamsungZSSD,
    MicronTLCSSD,
    GenericHDD,
    DRAM,
}

impl Device {
    fn read(&self) -> Duration {
        match self {
            Device::OptanePMem => Duration::from_nanos(270),
            Device::OptaneSSD => Duration::from_micros(9),
            Device::SamsungZSSD => Duration::from_micros(16),
            Device::MicronTLCSSD => Duration::from_micros(107),
            Device::GenericHDD => Duration::from_millis(10),
            Device::DRAM => Duration::from_nanos(80),
        }
    }
}

pub struct StorageStack {}

fn main() {
    println!("Hello, world!");
}
